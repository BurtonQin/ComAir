CXX=clang
CFLAGS=-O0 -Xclang -disable-O0-optnone -g -flto
LDFLAGS=-use-gold-plugin -Wl,-plugin-opt=save-temps
TARGET=target

HOME_PATH=/home/tzt77/Develop/ComAir
COUNTER_PASS=cmake-build-debug/lib/BBProfiling/libBBProfilingPass.so


all: null

null : target.bc
	opt -load ${HOME_PATH}/${COUNTER_PASS} -bb-profiling  target.bc > /dev/null

target.bc: mozilla35294.c
	${CXX} ${CFLAGS} mozilla35294.c -c -o target.bc

clean:
	rm -rf *.o *.bc